  "version": "0.1.0",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "babel-jest": "^25.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "react-test-renderer": "^16.12.0",
    "simple-progress-webpack-plugin": "^1.1.2",
    "webpack-dev-server": "^3.10.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/app/javascript/"
    ],
    "verbose": true,
    "testMatch": [
      "**/*.test.[jt]s?(x)"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules/",
      "<rootDir>/app/javascript/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/app/javascript/jest/setup.jest.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "test": "NODE_ENV=test jest --config package.json",
    "test:watch": "NODE_ENV=test jest --runInBand --config package.json --watch"
  }
}
